  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Lib Debug",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}\\dist/lib/server.js",
      "preLaunchTask": "tsc: build - tsconfig.json",
      "outFiles": ["${workspaceFolder}/dist/**/*.js"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Hello World Debug",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}\\dist/examples/helloworld/hello.js",
      "preLaunchTask": "tsc: build - examples/helloworld/tsconfig.json",
      "outFiles": ["${workspaceFolder}/dist/**/*.js"]
    }
  ]
}
